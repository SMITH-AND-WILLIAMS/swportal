.background {
  position: absolute;
  overflow: hidden;
  top: 0;
  left: 0;
  height: 100vh;
  width: 100vw;
  background-color:transparent;
  /* background-image: url(../img/sw-ho-me-full.jpg); */
  background-size: cover;
}
.background:before {
    content: "";
    display: block;
    position: absolute;
    background-color:transparent;
    background-image: url(sw-home-map.HASH_REF_6932b07674a0ee37d694e3e5afca1a43.png);
    background-size: cover;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    filter: url("#turbulence");   
  }
.background:after {
  content: "";
  display: block;
  position: absolute;
  background-image: url(sw-home-full.HASH_REF_16110bad65a73c3760f3576d1c6e1247.jpg);
  /* background: red; */
  background-size: cover;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  -webkit-mask: url(noise.HASH_REF_5d1d6c0ce30f761e640a6e9fb0fc02c5.png);
  mask: url(noise.HASH_REF_5d1d6c0ce30f761e640a6e9fb0fc02c5.png);
  -webkit-mask-position: var(--x) var(--y);
  mask-position: var(--x) var(--y);
  -webkit-mask-repeat: no-repeat;
  mask-repeat: no-repeat;
  backdrop-filter: blur(6010px);
}

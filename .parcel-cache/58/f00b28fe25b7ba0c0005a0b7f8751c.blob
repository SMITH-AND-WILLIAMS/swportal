:root{
  --primaryColor:#242021;
  --primaryColor0:#24202100;
}
body {
  overflow-x: hidden;
  background:var(--primaryColor);
}
header{
    perspective:1200px; 
    height:100vh;
    position:relative;
    background: url(c2cc673ffa2c74d9c628d47d5ccbaddd) center center no-repeat;
    background-size: cover;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}
h3.logo{
background: url(b1b6d3aa396adf5055d792d6bc7804da) center center no-repeat;
background-size: cover;
height:50px;
}
h3.logo span{
  opacity:0;  
}
header h2{

}
header:before{
  content:'';
  display:block;
  width:100%;
  max-width:1920px;  
  height:100%;
  background: url(943bfcbcfa80f5b62af0101b8fc3bec6) center center no-repeat;
  background-size: cover;
  position:absolute;
  z-index:1;
}
header:after{
  content:'';
  display:block;
  width:100%;
  height:20vh;
  position:absolute;
  bottom:-5px;
  left:0;  
  background:linear-gradient(var(--primaryColor0) 0%,var(--primaryColor) 90%);
}
.sea{
  content: "";
  display: block;
  position: absolute;
  background-color:transparent;
  background: url(590f39873719b8d5ab1f9fddbfb13727) center center no-repeat;
  background-size:cover;
  top: 0;
  left: 0;
  right:0;
  bottom:0;
  filter: url("#turbulence");   

}
.lightHouseReflection{
  content:'';
  display:block;
  width:100%;
  max-width:1920px;  
  height:100%;
  background: url(9d3c04fc1cde04a17d8b047e6178d1ae) center center no-repeat;
  background-size: cover;
  mix-blend-mode: hard-light;
  position:absolute;
  z-index:1;
  opacity:.85;
  filter: url("#turbulence");   
}

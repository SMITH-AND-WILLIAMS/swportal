:root{
  --primaryColor:#242021;
  --primaryColor0:#24202100;
}
body {
  overflow-x: hidden;
  background:var(--primaryColor);
}
header{
    perspective:1200px; 
    height:100vh;
    position:relative;
    background: url(sw-home-full.HASH_REF_63024d614315067a4200830877d6d2d1.webp) center center no-repeat;
    background-size: cover;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}
h3.logo{
background: url(logo.HASH_REF_9984a955a9f895fed2947df64b865dfd.svg) center center no-repeat;
background-size: cover;
height:55px;
width:700px;
margin:6vh 0 0 0;
transition: .5s;
}
h3.logo span, header h2 span{
  opacity:0;  
}
header h2{
  margin:20px 0 0 0;
background: url(baseline.HASH_REF_3db3d5a67c36029a0f81ab950389237a.svg) center center no-repeat;
background-size: cover;
width:372px;
height:15px;
transition: .5s;

}
@media (max-height:768px){
  h3.logo{
    width:500px;
    height:35px;
  }
  header h2{
    width:250px;
    height:10px;
  }
}
@media (min-width:1920px){
  h3.logo{
    width:800px;
    height:35px;
  }
  header h2{
    width:250px;
    height:10px;
  }
}
header:before{
  content:'';
  display:block;
  width:100%;
  max-width:1920px;  
  height:100%;
  background: url(sw-lighthouse.HASH_REF_b23708723c7467044d7f49796cb54d26.webp) center center no-repeat;
  background-size: cover;
  position:absolute;
  z-index:1;
}
header:after{
  content:'';
  display:block;
  width:100%;
  height:20vh;
  position:absolute;
  bottom:-5px;
  left:0;  
  background:linear-gradient(var(--primaryColor0) 0%,var(--primaryColor) 90%);
}
.sea{
  content: "";
  display: block;
  position: absolute;
  background-color:transparent;
  background: url(sw-sea.HASH_REF_5b6c2ff3722e3535bbe00e1ecc3d4492.webp) center center no-repeat;
  background-size:cover;
  top: 0;
  left: 0;
  right:0;
  bottom:0;
  filter: url("#turbulence");   

}
.lightHouseReflection{
  content:'';
  display:block;
  width:100%;
  max-width:1920px;  
  height:100%;
  background: url(sw-lighthouse-r.HASH_REF_908bcd92b6b1e06ab2256d40d36ee918.webp) center center no-repeat;
  background-size: cover;
  mix-blend-mode: hard-light;
  position:absolute;
  z-index:1;
  opacity:.85;
  filter: url("#turbulence");   
}

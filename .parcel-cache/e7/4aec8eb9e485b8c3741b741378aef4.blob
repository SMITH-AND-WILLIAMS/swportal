:root{
  --primaryColor:#242021;
  --primaryColor0:#24202100;
  --textColor:#FCE0C5;
}
body {
  overflow-x: hidden;
  background:var(--primaryColor);
  font-family: "Futura PT",Arial, Helvetica, sans-serif;
  color:var(--textColor);
}
header{
  perspective:1200px; 
  height:100vh;
  position:relative;
  background: url(c2cc673ffa2c74d9c628d47d5ccbaddd) center center no-repeat;  
  background-size: cover;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
h3.logo{
background: url(c064f45b73e9e093b6f46991e452e1da) center center no-repeat;
background-size: cover;
height:55px;
width:700px;
margin:6vh 0 0 0;
transition: .5s;
z-index:2;
position:relative;
animation: fadeDown .8s ease-in-out forwards;

}
@keyframes fadeDown{
  0%{
    opacity:0;
    top:-300px;
  }
  100%{
    opacity:1;
    top:0;
  }
}
@keyframes fadeUp{
  0%{
    opacity:0;
    top:100px;
  }
  100%{
    opacity:1;
    top:0;
  }
}
h3.logo span, header .baseline span{
  opacity:0;  
}
header .baseline{
margin:20px 0 0 0;
background: url(564d47a2a38b5de138eda4a8c20cd9de) center center no-repeat;
background-size: cover;
width:372px;
height:15px;
transition: .5s;
z-index:2;
opacity:0;
position:relative;
animation: fadeUp .5s .3s ease-in-out forwards;
}
@media (max-height:768px){
  h3.logo{
    width:500px;
    height:35px;
  }
  header .baseline{
    width:250px;
    height:10px;
  }
}
@media (min-width:1920px){
  h3.logo{
    width:900px;
    height:75px;
    margin-top:10vh
  }
  header .baseline{
    width:500px;
    height:21px;
  }
}
@media (max-width:768px){
  h3.logo{
    width:600px;
    height:46px;
  }
  header .baseline{
    width:320px;
    height:20px;
  }
}
@media (max-width:640px){
  h3.logo{
    width:450px;
    height:36px;
  }
  header .baseline{
    width:320px;
    height:20px;
  }
}
@media (max-width:460px){
  h3.logo{
    width:250px;
    height:20px;
  }
  header .baseline{
    width:220px;
    height:10px;
  }
}
header:before{
  content:'';
  display:block;
  width:100%;
  max-width:1920px;  
  opacity:0;
  height:100%;
  background: url(943bfcbcfa80f5b62af0101b8fc3bec6) center center no-repeat;
  background-size: cover;
  position:absolute;
  z-index:1;
  animation: flicker .5s 1s forwards;
}
header:after{
  content:'';
  display:block;
  width:100%;
  height:20vh;
  position:absolute;
  bottom:-5px;
  left:0;  
  background:linear-gradient(var(--primaryColor0) 0%,var(--primaryColor) 90%);
}
.sea{
  content: "";
  display: block;
  position: absolute;
  background-color:transparent;
  background: url(590f39873719b8d5ab1f9fddbfb13727) center center no-repeat;
  background-size:cover;
  top: 0;
  left: 0;
  right:0;
  bottom:0;
  filter: url("#turbulence");   

}
.lightHouseReflection{
  content:'';
  display:block;
  width:100%;  
  height:100%;
  background: url(9d3c04fc1cde04a17d8b047e6178d1ae) center center no-repeat;
  background-size: cover;
  mix-blend-mode: hard-light;


  filter: url("#turbulence");   
}
.lh{
  width:100%;
  max-width:1920px;  
  height:100%;
  position:absolute;
  z-index:1;
  opacity:0;
  animation: flicker2 .5s 1s forwards;
}
@keyframes flicker {
  62.999%,
  64%,
  64.999%,
  
  100% {
    opacity: 0.8;
    filter: brightness(1);
  }
  0%,
  19.999%,
  22%,
  34%,
  35% {
    opacity: 0;
    filter: brightness(1);
  }
  20%,
  21.999%,
  34.999%,
  63%,
  63.999%,
  65%,
  69.999% {
    opacity: .85;
    filter: brightness(1);
  }
}
@keyframes flicker2 {
  62.999%,
  64%,
  64.999%,
  70%,
  100% {
    opacity: 0.55;
    filter: brightness(1);
  }
  0%,
  19.999%,
  22%,
  34%,
  35% {
    opacity: 0;
    filter: brightness(1);
  }
  20%,
  21.999%,
  34.999%,
  63%,
  63.999%,
  65%,
  69.999% {
    opacity: .55;
    filter: brightness(1);
  }
}
